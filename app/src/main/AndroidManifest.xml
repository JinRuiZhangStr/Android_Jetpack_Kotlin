<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eflagcomm.android_jetpack_kotlin">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".JetpackApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.Android_Jetpack_Kotlin">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".lifeCycle.LifeCycleActivity"></activity>

        <activity android:name=".viewmodel.ViewModelActivity"></activity>

        <activity android:name=".sndemo.PcListActivity"></activity>

        <activity android:name=".sndemo.AddPcUnitActivity"></activity>

        <activity android:name=".livedata.LiveDataActivity"></activity>

        <activity android:name=".room.RoomActivity"></activity>

        <activity android:name=".sndemo.VideoPlayerActivity"></activity>

        <activity android:name=".sndemo.LeftSlipListActivity"></activity>

        <activity android:name=".hilt.HiltActivity"></activity>
    </application>

</manifest>