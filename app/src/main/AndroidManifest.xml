<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eflagcomm.android_jetpack_kotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".JetpackApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android_Jetpack_Kotlin"
        android:usesCleartextTraffic="true">
        <activity android:name=".databind.DataBindActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".lifeCycle.LifeCycleActivity" />
        <activity android:name=".viewmodel.ViewModelActivity" />
        <activity android:name=".sndemo.PcListActivity" />
        <activity android:name=".sndemo.AddPcUnitActivity" />
        <activity android:name=".livedata.LiveDataActivity" />
        <activity android:name=".room.RoomActivity" />
        <activity android:name=".sndemo.VideoPlayerActivity" />
        <activity android:name=".sndemo.LeftSlipListActivity" />
        <activity android:name=".hilt.HiltActivity" />
        <activity android:name=".coroutines.CoroutinesActivity" />
    </application>

</manifest>